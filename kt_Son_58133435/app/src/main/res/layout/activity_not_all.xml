<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:context=".NotAllActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="10dp"
        android:layout_weight="1">

        <TextView
            android:id="@+id/txtStt3"
            android:layout_width="75dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="1" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">

            <TextView
                android:id="@+id/txtTenMT3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Thiết kế web" />

            <TextView
                android:id="@+id/txtNgayThi3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="17/6/2019" />
        </LinearLayout>
    </LinearLayout>

    <TextView
        android:id="@+id/textView53"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#111111" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="10dp"
        android:layout_weight="1">

        <TextView
            android:id="@+id/txtStt4"
            android:layout_width="75dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="2" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">

            <TextView
                android:id="@+id/txtTenMT4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Công nghệ phần mềm" />

            <TextView
                android:id="@+id/txtNgayThi4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="19/6/2019" />
        </LinearLayout>
    </LinearLayout>

    <TextView
        android:id="@+id/textView54"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#111111" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="10dp"
        android:layout_weight="1">

        <TextView
            android:id="@+id/txtStt"
            android:layout_width="75dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="3" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">

            <TextView
                android:id="@+id/txtTenMT"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Trí tuệ nhân tạo" />

            <TextView
                android:id="@+id/txtNgayThi"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="21/6/2019" />
        </LinearLayout>
    </LinearLayout>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#111111" />

</LinearLayout>